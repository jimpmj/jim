        <typeAlias type="com.jim.im.db.entity.group.Group" alias="Group"/>
        <typeAlias type="com.jim.im.db.entity.group.User" alias="User"/>
        <typeAlias type="com.jim.im.db.entity.group.GroupUserMap" alias="GroupUserMap"/>
